	<pron value="jié">
		<sense tag="名" vernacular="竹节">
			<exam article="《晋书·杜预传》" ref="譬如破竹，数节之后，皆迎刃而解。" />
		</sense>
		<sense tag="名" vernacular="泛指植物的茎节；植物枝干交接之处">
			<exam article="《易·说卦》" ref="其于木也，为坚多节。" />
		</sense>
		<sense tag="名" vernacular="人及动物的骨节、关节">
			<exam article="《庖丁解牛》" ref="彼节者有间，而刀刃者无厚。" />
		</sense>
		<sense tag="名" vernacular="时节；季节">
			<exam article="《雨霖铃》" ref="更那堪冷落清秋节。" />
		</sense>
		<sense tag="名" vernacular="节日">
			<exam article="《九月九忆山东兄弟》" ref="每逢佳节倍思亲。" />
		</sense>
		<sense tag="名" vernacular="符节；旌节">
			<exam article="《江城子·密州出猎》" ref="持节云中，何日遣冯唐？" />
		</sense>
		<sense tag="名" vernacular="礼节；礼仪">
			<exam article="《论积贮疏》" ref="管子曰：‘仓廪实而知礼节。’" />
		</sense>
		<sense tag="名" vernacular="常规惯例">
			<exam article="《狱中杂记》" ref="寝食违节。" />
		</sense>
		<sense tag="名" vernacular="气节；节操">
			<exam article="《陈情表》" ref="本图宦达，不矜名节。" />
		</sense>
		<sense tag="名" vernacular="贞节；贞操">
			<exam article="《孔雀东南飞》" ref="君既为府吏，守节情不移。" />
		</sense>
		<sense tag="名" vernacular="关键；要害">
			<exam article="《察传》" ref="夫乐，天之精也，得失之节也。" />
		</sense>
		<sense tag="名" vernacular="章节；段落">
			<exam article="《杂纂》" ref="文无句读，书少章节。" />
		</sense>
		<sense tag="名" vernacular="节度；法度；分寸">
			<exam article="《荀子·成相》" ref="言有节。" />
		</sense>
		<sense tag="名" vernacular="一种用竹子编成的可拍击成声的乐器">
			<exam article="《蜀都赋》" ref="巴姬弹弦，汉女击节。" />
		</sense>
		<sense tag="名" vernacular="节拍；节奏">
			<exam article="《促织》" ref="每闻琴瑟之声，则应节而舞。" />
		</sense>
		<sense tag="动" vernacular="节制；节约">
			<exam article="《长亭送别》" ref="趁程途节饮食，顺时自保揣身体。" />
			<exam article="《训俭示康》" ref="小人寡欲则能谨身节用，远罪丰家。" />
		</sense>
		<sense tag="形" vernacular="简明；简要">
			<exam article="《答韦中立论师道书》" ref="疏之欲其通，廉之欲其节。" />
		</sense>
		<sense tag="名" vernacular="斗拱；屋柱上端承梁的方形木头">
			<exam article="《训俭示康》" ref="（管仲）山节藻棁，孔子鄙其小器。" />
		</sense>
	</pron>
</character>
