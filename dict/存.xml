			<exam article="《归去来兮辞》" ref="三径就荒，松菊犹存。" />
			<exam article="《捕蛇者说》" ref="视其缶，而吾蛇尚存。" />
			<exam article="《师说》" ref="道之所存，师之所存也。" />
		</sense>
		<sense tag="动" vernacular="生存，活着">
			<exam article="《冯谖客孟尝君》" ref="齐有冯谖者，贫乏不能自存。" />
			<exam article="《石壕吏》" ref="存者且偷生，死者长已矣！" />
		</sense>
		<sense tag="动" vernacular="关心，想念">
			<exam article="《屈原列传》" ref="其存君兴国而欲反复之，一篇之中三致志焉。" />
			<exam article="《订鬼》" ref="凡天地之间，有鬼，非人死精神为之也，皆人思念存想之所致也。" />
		</sense>
		<sense tag="动" vernacular="慰问，抚恤">
			<exam article="《信陵君窃符救赵》" ref="臣乃市井鼓刀屠者，而公子亲数存之。" />
			<exam article="《隆中对》" ref="民殷国富而不知存恤。" />
		</sense>
